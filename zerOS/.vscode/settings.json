{
    "nasm.outputFormat": "elf64",
    "editor.detectIndentation": true,
    "files.associations": {
        "*.h": "c",
        "*.pl": "prolog",
        "*.pro": "prolog",
        "*.cog_processed": "c",
        "rope": "cpp",
        "*.ld": "linkerscript",
        "*gmsl": "makefile",
        "*.sd": "c",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "string": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "string_view": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "ratio": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "typeinfo": "cpp",
        "variant": "cpp",
        "__verbose_abort": "cpp",
        "__config": "cpp",
        "filesystem": "cpp",
        "ostream": "cpp",
        "print": "cpp",
        "format": "cpp",
        "mutex": "cpp",
        "ranges": "cpp",
        "span": "cpp",
        "text_encoding": "cpp",
        "*.def": "c",
        "*.in": "c",
        "*.tpp": "c"
    },
    "C_Cpp.default.cppStandard": "gnu++23",
    "C_Cpp.default.cStandard": "gnu23",
    "C_Cpp.default.intelliSenseMode": "linux-clang-x64",
    //"C_Cpp.intelliSenseCacheSize": 10240,
    //"C_Cpp.intelliSenseMemoryLimit": 8192,
    //"C_Cpp.workspaceParsingPriority": "highest",
    "C_Cpp.doxygen.generatedStyle": "/**",
    "C_Cpp.doxygen.generateOnType": true,
    "C_Cpp.doxygen.sectionTags": [
        "attention",
        "tparam",
        "param",
        "result",
        "returns",
        "retval",
        "exception",
        "deprecated",
        "warning",
        "note",
        "pre",
        "post",
        "invariant"
    ],
    "C_Cpp.files.exclude": {
        "**/.vscode": true,
        "**/.vs": true,
        //"**/toolchain/src/**": true
        "**/toolchain/**": true
    },
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.intelliSenseEngineFallback": "enabled",
    "C_Cpp.autocomplete": "default",
    /* "C_Cpp.exclusionPolicy": "checkFilesAndFolders", */
    "python.analysis.typeCheckingMode": "standard",
    "C/C++ Include Guard.Macro Type": "Filepath",
    "C/C++ Include Guard.Header Extensions": [
        ".h",
        ".hpp",
        ".h++",
        ".hh"
    ],
    "C/C++ Include Guard.Suffix": "_INCLUDED_",
    "C/C++ Include Guard.Path Skip": 2,
    "C/C++ Include Guard.Prefix": "zerOS_",
    "editor.tabCompletion": "off",
    "C_Cpp.intelliSenseMemoryLimit": 4096,
    "C_Cpp.workspaceParsingPriority": "medium",
    "latex-workshop.latex.autoBuild.run": "never",
    "clang-format.executable": "clang-format",
    "clang-format.style": "file",
    "clang-format.language.c.enable": true,
    "clang-format.language.cpp.enable": true,
    "clang-format.assumeFilename": "${workspaceFolder}/.clang-format",
    "[c]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    /* "rust-analyzer.server.path": "/home/axel/.cargo/bin/rust-analyzer", */
    /* "rust-analyzer.procMacro.server": "/opt/rust-bin-9999/libexec/rust-analyzer-proc-macro-srv", */
    "rust-analyzer.check.workspace": false,
    "rust-analyzer.cargo.buildScripts.enable": true,
    "rust-analyzer.diagnostics.enable": true,
    "rust-analyzer.hover.memoryLayout.niches": true,
    "rust-analyzer.hover.memoryLayout.enable": true,
    "rust-analyzer.hover.memoryLayout.alignment": "both",
    "rust-analyzer.hover.memoryLayout.offset": "both",
    "rust-analyzer.hover.memoryLayout.size": "both",
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.lens.enable": true,
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
    "rust-analyzer.typing.triggerChars": "=.{(<>",
    "rust-analyzer.signatureInfo.documentation.enable": true,
    "rust-analyzer.signatureInfo.detail": "full",
    "rust-analyzer.rustfmt.extraArgs": [
        "--config-path",
        "${workspaceFolder}/rustfmt.toml",
    ],
    "rust-analyzer.inlayHints.implicitDrops.enable": true,
    "rust-analyzer.cargo.target": "/home/axel/tmp/x86_64-unknown-kernel.json",
    "rust-analyzer.cargo.extraEnv": {
        "ZEROS_TARGET_CPU": "skylake",
        /* "RUSTUP_TOOLCHAIN": "stable", */
        /* "__CARGO_TEST_CHANNEL_OVERRIDE_DO_NOT_USE_THIS": "nightly" */
    },
    //"rust-analyzer.cargo.extraArgs": [
    //    "+nightly"
    //],
    /* "rust-analyzer.cargo.sysroot": "/home/axel/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu", */
    "rust-analyzer.procMacro.attributes.enable": true,
}